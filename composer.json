{
	"name" : "avored/framework",
	"description" : "AvoRed Laravel E commerce Framework",
	"keywords" : [
		"framework",
		"cart",
		"laravel",
		"e commerce",
		"laravel5",
		"shop",
		"shopping-cart",
		"e-commerce",
		"shopping cart",
		"e commerce"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "Purvesh  ",
			"email" : "ind.purvesh@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : ">=7.1.3",
		"symfony/yaml" : "^4.0",
		"laravel/framework" : "5.7.*",
		"barryvdh/laravel-dompdf": "0.8.*",
		"laravel/passport": "7.0.*",
		"stripe/stripe-php": "^6.3",
		"netojose/laravel-bootstrap-4-forms": "^2.0",
		"smartins/passport-multiauth": "^4.0",
		"cviebrock/eloquent-sluggable": "^4.6.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~7.0",
		"orchestra/testbench" : "~3.7",
		"mockery/mockery" : "1.2.*"
	},
	"autoload" : {
		"classmap": [
            "database/factories"
        ],
		"psr-4" : {
			"AvoRed\\Framework\\" : "src/"

		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"AvoRed\\Framework\\Tests\\" : "tests/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"AvoRed\\Framework\\Provider"
			],
			"aliases" : {
				"AdminMenu" : "AvoRed\\Framework\\AdminMenu\\Facade",
				"AdminConfiguration" : "AvoRed\\Framework\\AdminConfiguration\\Facade",
				"Breadcrumb" : "AvoRed\\Framework\\Breadcrumb\\Facade",
				"Cart" : "AvoRed\\Framework\\Cart\\Facade",
				"DataGrid" : "AvoRed\\Framework\\DataGrid\\Facade",
				"Image" : "AvoRed\\Framework\\Image\\Facade",
				"Menu" : "AvoRed\\Framework\\Menu\\Facades\\Menu",
				"Payment" : "AvoRed\\Framework\\Payment\\Facade",
				"Permission" : "AvoRed\\Framework\\Permission\\Facade",
				"Shipping" : "AvoRed\\Framework\\Shipping\\Facade",
				"Tabs" : "AvoRed\\Framework\\Tabs\\Facade",
				"Theme" : "AvoRed\\Framework\\Theme\\Facade",
				"Widget" : "AvoRed\\Framework\\Widget\\Facade"
			}
		}
	},
	"homepage" : "https://www.avored.com",
	"support" : {
		"email" : "ind.purvesh@gmail.com",
		"issues" : "https://www.avored.com/discussion",
		"forum" : "https://www.avored.com/discussion",
		"wiki" : "https://www.avored.com/docs",
		"source" : "https://www.github.com/avored/framework"
	}
}
