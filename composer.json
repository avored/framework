{
    "name": "avored/framework",
    "description": "AvoRed a laravel ecommerce framework",
    "keywords": [
        "AvoRed",
        "laravel",
        "framework"
    ],
    "homepage": "https://github.com/avored/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Purvesh",
            "email": "ind.purvesh@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.4",
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.2",
        "orchestra/testbench": "7.5",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.23"
    },
    "autoload": {
        "psr-4": {
            "AvoRed\\Framework\\": "src/",
            "AvoRed\\Framework\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvoRed\\Framework\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm --show-info=true",
        "phpunit": "vendor/bin/phpunit",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html build/coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvoRed\\Framework\\AvoRedServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
